.import some-external-value
.export main

.label main
load 400 r0
loadh 200 r1
loadw 300 r2
loadhw 300 r4
savew r2 200
savehw r2 204

sub r0 r1 r0
add r0 2  r0
sub r0 1  r0
save r0 400
saveh r0 200
jgz r0 another-function

.label another-function
add r1 some-external-value r1
jgz r1 and-another-function

.label and-another-function
add r0 1 r1
int
brk
